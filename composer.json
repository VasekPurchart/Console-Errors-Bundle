{
	"name": "vasek-purchart/console-errors-bundle",
	"description": "Logging of exceptions and error codes for Symfony Console",
	"keywords": ["console", "error", "exception", "debug", "logging", "symfony", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~5.6|~7.0",
		"psr/log": "~1.0",
		"symfony/config": "~2.6|~3.0",
		"symfony/console": "~2.6|~3.0",
		"symfony/dependency-injection": "~2.6|~3.0",
		"symfony/http-kernel": "~2.6|~3.0",
		"symfony/yaml": "~2.6|~3.0"
	},
	"require-dev": {
		"consistence/coding-standard": "0.12",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"matthiasnoback/symfony-dependency-injection-test": "1.1",
		"phing/phing": "2.16",
		"phpunit/phpunit": "5.7.5"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\ConsoleErrorsBundle\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\ConsoleErrorsBundle\\": "tests"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
