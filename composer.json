{
	"name": "vasek-purchart/console-errors-bundle",
	"description": "Logging of exceptions and error codes for Symfony Console",
	"keywords": ["console", "error", "exception", "debug", "logging", "symfony", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"psr/log": "~1.0",
		"symfony/config": "~4.0",
		"symfony/console": "~4.0",
		"symfony/dependency-injection": "~4.0",
		"symfony/http-kernel": "~4.0",
		"symfony/yaml": "~4.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"matthiasnoback/symfony-dependency-injection-test": "3.1",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "7.5.1"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\ConsoleErrorsBundle\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\ConsoleErrorsBundle\\": "tests"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
